{
	"name" : "oxyfony/user-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony Oxygen User Bundle",
	"keywords" : [
		"Oxygen",
		"symfony",
		"user bundle"
	],
	"homepage" : "http://oxygen.soletic.org",
	"license" : "MIT",
	"authors" : [{
			"name" : "Oxygen Community",
			"homepage" : "http://oxygen.soletic.org"
		}
	],
	"require" : {
		"php" : ">=5.3.2",
		"symfony/symfony" : ">=2.3",
		"sensio/framework-extra-bundle" : ">=2.3",
		"twig/extensions" : ">=1.0",
		"symfony/assetic-bundle" : ">=2.3",
		"doctrine/doctrine-bundle" : "~1.2",
		"doctrine/orm" : "~2.2,>=2.2.3",
		"friendsofsymfony/user-bundle" : "~1.3"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~3.7"
